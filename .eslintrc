{
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": 0,
    "function-paren-newline": 0,
    "global-require": 0,
    "no-confusing-arrow": 0,
    "no-nested-ternary": 0,
    "object-curly-newline": 0,
    "no-mixed-operators": 0,
    "import/no-extraneous-dependencies": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "no-unsafe-finally": 1,
    "no-useless-concat": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
