function map_initialize(){mapInit=!0}function openMap(){if(mapInit){var e=document.getElementById("map_win");null!=e?"block"==e.style.display?closeMap():(closeHolder(),$("#map-link").addClass("active"),$("#map_win").slideDown("400",function(){mapInitContent||$.get({url:"/map",headers:{"X-Requested-With":"XMLHttpRequest"}},function(e){$("#map_content").html(e),mapInitContent=!0,initialize()})})):alert("Объект еще не загрузился, попробуйте чуть позже.")}else alert("Нет доступа к карте maps.googleapis.com, попробуйте позже.")}function closeMap(){mapInitContent&&($("#map_win").slideUp("400",function(){$("#map-link").removeClass("active")}),$("#content div").hasClass("main-banner")&&$(initMainBanner))}var mapInit=!1,mapInitContent=!1,script=document.createElement("script");script.src="https://maps.googleapis.com/maps/api/js?libraries=geometry,places&language=ru&v=3.exp&callback=map_initialize&key=AIzaSyC0_eHBxo3X_oYRsvfswsBF5jHhs3oQRqo",script.type="text/javascript",document.getElementsByTagName("head")[0].appendChild(script),$(document).click(function(e){if("none"!=$("#map_win").css("display")){var n=$(e.target),i=n[0].id;("header"==i||"footer"==i||"inner-wrapper"==i||n.closest("#ibank-bg").length>0||n.closest(".header-phone").length>0)&&closeMap()}});var isMobile={Android:function(){return navigator.userAgent.match(/Android/i)},BlackBerry:function(){return navigator.userAgent.match(/BlackBerry/i)},iOS:function(){return navigator.userAgent.match(/iPhone|iPod/i)},Opera:function(){return navigator.userAgent.match(/Opera Mini/i)},Windows:function(){return navigator.userAgent.match(/IEMobile/i)},any:function(){return isMobile.Android()||isMobile.BlackBerry()||isMobile.iOS()||isMobile.Opera()||isMobile.Windows()}};